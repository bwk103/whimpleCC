<% provide(:title, 'Events') %>

<div id="events-index">
  <section class="title">
     <h1>Events</h1>
  </section>
  <%= render 'shared/flash' %>
  <% if @events %>
    <section class="event-list">
      <% @events.each do | event | %>
        <%= render 'event', { event: event } %>
      <% end %>
    </section>
  <% end %>
</div>
